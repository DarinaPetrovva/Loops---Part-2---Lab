using System;

namespace demo
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            int sum = 0;
            for (int i = 0; i < n; i++)
            {
                int numbers = int.Parse(Console.ReadLine());
                string st = Console.ReadLine();
                do
                {
                    sum += numbers;
                } while (st != "Stop");
            }
            Console.WriteLine(sum);
            
            
        }
    }
}
